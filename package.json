{
  "name": "demo-mcp-registry",
  "version": "1.0.0",
  "description": "A static registry for Model Context Protocol (MCP) servers, hosted on GitHub Pages. Browse and install MCP servers directly from the web interface or integrate with the API.",
  "main": "script.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "migrate": "cd backend && npm run migrate",
    "seed": "cd backend && npm run seed",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tdupoiron-org/demo-mcp-registry.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/tdupoiron-org/demo-mcp-registry/issues"
  },
  "homepage": "https://github.com/tdupoiron-org/demo-mcp-registry#readme",
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
